set -e
set -u

DIR="$( dirname "$0" )"
cd ${DIR}

pwd
rm -rf .git

git init >/dev/null
echo hi > 'a b'
git add -- 'a b'
git commit -m"a b file added" >/dev/null
echo changeA >> 'a b'
git stash >/dev/null
echo changeB >> 'a b'
git add 'a b'
(git stash pop >/dev/null && exit 1) || echo "Conflict set up successfully"
# Set up of conflict over

../${resolve_trivial_conflicts}

rm -rf .git 'a b'
