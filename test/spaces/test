set -e
set -u
set -x

DIR="$( dirname "$0" )"
cd ${DIR}

pwd
rm -rf .git

git init
echo hi > 'a b'
git add -- 'a b'
git commit -m"a b file added"
echo changeA >> 'a b'
git stash
echo changeB >> 'a b'
git add 'a b'
(git stash pop && exit 1) || echo "Conflict set up successfully"
# Set up of conflict over

../${resolve_trivial_conflicts}

rm -rf .git 'a b'
